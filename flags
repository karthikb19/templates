co() { g++ -std=c++17 -DLOCAL -O2 -Wall  -Wextra -pedantic -Wshadow -Wformat=2 -Wfloat-equal -Wconversion -Wlogical-op -Wshift-overflow=2 -Wduplicated-cond -Wcast-qual -Wcast-align -Wno-unused-result -Wno-sign-conversion -o $1.out $1.cpp -fsanitize=address -fsanitize=undefined -fsanitize=address -fsanitize=undefined -fsanitize=float-divide-by-zero -fsanitize=float-cast-overflow -fno-sanitize-recover=all -fstack-protector-all -D_FORTIFY_SOURCE=2 -D_GLIBCXX_DEBUG; }
cf() { g++ -std=c++17 -DLOCAL -O2 -o $1.out $1.cpp -Wall; }
